<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>stingray_utils</name>
  <version>0.1.0</version>
  <description>Package contains different helper utilities of the Stingray platform.</description>

  <maintainer email="rentgeny@mail.ru">Timur Akhtyamov</maintainer>
  <maintainer email="myaghkov16@gmail.com">Artyom Davtyan</maintainer>
  <maintainer email="vladushked@yandex.ru">Vladislav Plotnikov</maintainer>

  <license>MIT</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <build_export_depend>std_msgs</build_export_depend>
  <build_export_depend>rclpy</build_export_depend>
  <build_export_depend>cv_bridge</build_export_depend>
  <build_export_depend>sensor_msgs</build_export_depend>
  <build_export_depend>image_transport</build_export_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
